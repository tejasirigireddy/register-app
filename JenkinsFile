pipeline {
    agent any
    
    tools{
        jdk 'Java 17'
        maven 'Maven3'
    }
    stages{
        stage('cleanup workspace'){
            steps{
                cleanWs()
            }
        }
    }
    stage('code'){
        steps{
            git branch: 'main' 
            url: 'https://github.com/tejasirigireddy/register-app.git'
        }
    }
    stage('Build'){
        steps{
            sh 'mvn clean package'
        }
    }
    stage('MavenTest')
    {
        steps{
            sh 'mvn test'
        }
    }
}
